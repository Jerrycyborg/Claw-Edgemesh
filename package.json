{
  "name": "openclaw-edgemesh",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "node --import tsx --test src/test.ts src/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "verify:quick": "npm test && npm run build",
    "release:gate": "npm test && npm run build",
    "dev:control": "tsx src/control-plane.ts",
    "dev:node": "tsx src/edge-node.ts",
    "start:control": "node dist/control-plane.js",
    "start:node": "node dist/edge-node.js",
    "prepare": "husky"
  },
  "dependencies": {
    "fastify": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.23.0"
  }
}
